# Environment Configuration
DEBUG=True
ENVIRONMENT=development

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Database Configuration (Legacy - Supabase recommended)
DATABASE_URL=postgresql://ros_user:ros_password@localhost:5432/ros_db
TEST_DATABASE_URL=postgresql://ros_user:ros_password@localhost:5432/ros_test_db

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Route Optimization System
VERSION=1.0.0
DESCRIPTION=A modern, cloud-based route optimization service

# Security
SECRET_KEY=your-secret-key-here-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30

# External Services
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
MAPBOX_ACCESS_TOKEN=your-mapbox-access-token

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100

# Logging
LOG_LEVEL=INFO
